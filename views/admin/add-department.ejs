               <%- include('./includes/header.ejs') %>
     
         <!-- Side Bar -->
             <%- include('./includes/sidebar.ejs') %>


      <!-- ============================================================== -->
      <!-- Page wrapper  -->
      <!-- ============================================================== -->
      <div class="page-wrapper">
                <!-- Page-Title -->
                <%- include('./includes/page-title.ejs') %>
        
        <!-- ============================================================== -->
        <!-- Container fluid  -->
        <!-- ============================================================== -->
        <div class="container-fluid">

            <!-- Put Message -->
            <% if(locals.message) {%>
                <div class="alert <%= alert %> alert-dismissible fade show" role="alert">
                        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                        </button>
                        <strong>Message: <%= message %></strong> 
                </div>
                <% } %>
               
                    <!-- Put page -->
                    
                           <!-- content -->

                           <div class="card">
                            <div class="card-body">
                
                                <h4 class="card-title">Add Department</h4>
                                
                                
                                <form method="post" action="/admin/add-department" class="form-submit" id="newStudentForm">
                                    <div class="form-group mb-3">
                                        <label for="department" class="col-3 col-form-label">Department</label>
                                        <div class="col-12">
                                            <input type="text" class="form-control" name="department" placeholder="Enter Department" required>
                                        </div>
                                    </div>

                                    <div class="form-group mb-3">
                                        <label for="description" class="col-3 col-form-label">Description</label>
                                        <div class="col-12">
                                           <textarea class="form-control" name="description" id="" cols="30" rows="4"></textarea>
                                        </div>
                                    </div>
                                  
                                    <div class="form-group mb-0 ">
                                        <div class="col-12">
                                            <button type="submit" id="newStudentFormBtn" name="add-department" class="btn btn-info waves-effect waves-light submit-btn"><i class="fa fa-plus"></i> Add New Department</button>
                                        </div>
                                    </div>
                                </form>
                            </div> <!-- end card-body-->
                        </div> <!-- end card-->   
           <!-- End Put page -->        
            
               
        </div>
        <!-- ============================================================== -->
        <!-- End Container fluid  -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <%- include('./includes/footer.ejs') %>
